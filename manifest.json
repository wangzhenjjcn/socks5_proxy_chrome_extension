{
  "manifest_version": 2,
  "name": "SOCKS5 代理插件",
  "version": "1.0",
  "description": "一个简单的SOCKS5代理插件",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon_gray.png"
    },
    "default_popup": "popup.html",
    "default_title": "SOCKS5 代理插件"
  },
  "permissions": [
    "proxy",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
