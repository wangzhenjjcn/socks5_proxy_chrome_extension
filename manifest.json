{
    "manifest_version": 3,
    "name": "王师傅黑科技",
    "version": "1.3",
    "description": "王师傅的科技与狠活",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": ["proxy", "storage"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  